<%= form_tag logins_path, :remote => true do %>

  <%= text_field_tag :username, nil, placeholder: "Username" %>
  <%= password_field_tag :password, nil, placeholder: "Password" %>
  
  <%= submit_tag "Log In" %>
<% end %>

<%= form_tag(:root, method: :get, remote: true) do %>
  <%= submit_tag "Cancel" %>
<% end %>